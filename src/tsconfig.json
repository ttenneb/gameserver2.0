{
  "compilerOptions": {
    "outDir": "../dist",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "removeComments": false,
    "moduleResolution": "node",
    "declaration": true,
    "allowSyntheticDefaultImports": true
  }
}